{
    "name": "spiral/tokenizer",
    "type": "library",
    "description": "Static Analysis: Class and Invocation locators",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/tokenizer"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "spiral/core": "^2.9",
        "spiral/logger": "^2.9",
        "symfony/finder": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Tokenizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Tokenizer\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.9.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
